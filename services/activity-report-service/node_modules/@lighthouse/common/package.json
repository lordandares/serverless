{
  "name": "@lighthouse/common",
  "version": "4.15.13",
  "description": "",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "scripts": {
    "build": "yarn clear && yarn build:es && yarn build:node",
    "build:es": "yarn clear:es && babel src --out-dir lib --extensions '.js,.ts' --ignore 'src/**/fixtures.js,src/**/fixtures/*,src/**/*.story.js,src/**/*.story.ts,src/**/*.test.js,src/**/*.test.ts' --source-maps",
    "build:node": "yarn clear:node && BABEL_ENV=node babel src --out-dir dist --extensions '.js,.ts' --ignore 'src/**/fixtures.js,src/**/fixtures/*,src/**/*.story.js,src/**/*.story.ts,src/**/*.test.js,src/**/*.test.ts'",
    "clear": "yarn clear:es && yarn clear:node",
    "clear:es": "rimraf lib/",
    "clear:node": "rimraf dist/",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src/",
    "lint:fix": " yarn lint --fix",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build",
    "preversion": "yarn test",
    "release:auto": "auto shipit --verbose",
    "storybook:html": "start-storybook -p 9001 -c .storybook-html -s ./src",
    "storybook:react": "start-storybook -p 9002 -c .storybook-react -s ./src",
    "test": "TZ=UTC jest --config jest.config.json",
    "test:ci": "yarn lint && yarn type-check && yarn test --coverage",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "validate:circleci": "circleci config validate -c .circleci/config.yml",
    "version": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lighthouse-io/common.git"
  },
  "author": "Will McClellan",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Lighthouse-io/common/issues"
  },
  "homepage": "https://github.com/Lighthouse-io/common#readme",
  "dependencies": {
    "@lighthouse/abab": "^0.0.6",
    "bluebird": "^3.7.2",
    "fetch-ponyfill": "^6.1.0",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.28",
    "react-html-parser": "^2.0.2",
    "storybook": "^5.3.14",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-notes": "^5.3.14",
    "@storybook/html": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.7.5",
    "@types/yup": "^0.26.32",
    "auto": "^7.4.4",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "jest": "^24.1.0",
    "jest-junit": "^5.1.0",
    "lint-staged": "^7.2.0",
    "mockdate": "^2.0.2",
    "nock": "^12.0.2",
    "prettier": "^1.19.1",
    "react-visjs-timeline": "^1.6.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.8.3",
    "vis-timeline": "^5.1.0"
  }
}
